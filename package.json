{
  "name": "todolist",
  "version": "1.0.0",
  "description": "Simple todo list build with MERN Stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix public",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "npm start --prefix public",
    "server": "concurrently \"npm run dev\" \"npm run client\" "
  },
  "author": "Maavia Asghar",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.15",
    "nodemon": "^2.0.6"
  }
}
